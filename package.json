{
  "name": "kontent-custom-element-samples",
  "description": "Gallery of custom elements for Kentico Kontent.",
  "version": "0.2.0",
  "author": "Chris Jennings <devrel@kentico.com>",
  "dependencies": {
    "gatsby": "^2.24.3",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-google-analytics": "^2.1.38",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-json": "^2.2.26",
    "gatsby-transformer-sharp": "^2.3.14",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
